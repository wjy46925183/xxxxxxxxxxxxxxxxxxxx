<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
    android:background="#efefef">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_myservice"
        android:layout_marginTop="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginLeft="12dp">
        <ImageView
            android:id="@+id/iv_item_myserice"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginLeft="@dimen/margin_15"
            android:layout_marginTop="@dimen/margin_7"
            android:layout_marginBottom="@dimen/margin_7"
            android:scaleType="fitXY"
            android:src="@mipmap/icon_default_user"/>

        <TextView
            android:id="@+id/tv_position"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/iv_item_myserice"
            android:layout_marginLeft="@dimen/margin_15"
            android:layout_toRightOf="@+id/iv_item_myserice"
            android:text="服务员"
            android:textColor="@color/textColor_black_light01"
            android:textSize="@dimen/text_size_15"/>

        <TextView
            android:id="@+id/tv_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_15"
            android:layout_toRightOf="@+id/iv_item_myserice"
            android:text="100元/天"
            android:layout_alignBottom="@+id/iv_item_myserice"
            android:textColor="@color/app_color_yellow"
            android:textSize="@dimen/text_size_14"/>
    </RelativeLayout>

</FrameLayout>