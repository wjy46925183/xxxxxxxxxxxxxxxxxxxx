    public String getDemandTypeStr(){
        String demandTypeStr = "";
        //需求类型(1.工作日,2.双休日,3.计件)
        switch (demandType) {
            case 1:
                demandTypeStr = "工作日";
                break;
            case 2:
                demandTypeStr = "双休日";
                break;
            case 3:
                demandTypeStr = "计件";
                break;
        }
        return demandTypeStr;
    }

