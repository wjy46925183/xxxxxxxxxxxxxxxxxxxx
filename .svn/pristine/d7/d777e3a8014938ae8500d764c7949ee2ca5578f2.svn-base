<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme.NoActionBar.Start">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="toolbarStyle">@style/inc_ClubToolbar</item>
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowBackground">@color/red</item>-->
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="toolbarStyle">@style/inc_ClubToolbar</item>
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowAnimationStyle">@style/fade</item>-->
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <!--<item name="colorAccent">@color/colorPrimaryDark</item>-->
        <item name="toolbarStyle">@style/inc_ClubToolbar</item>
        <item name="android:actionBarSize">@dimen/inc_height_6_80</item>
        <item name="android:windowBackground">@color/inc_app_bg_page</item>
    </style>

    <style name="ClubToolbar" parent="Widget.AppCompat.Toolbar">
        <item name="contentInsetStart">0dp</item>
        <!-- 设置该属性解决空白部分-->
    </style>
    <style name="TextAppearance.Design.Tab.Custom" parent="TextAppearance.AppCompat.Button">
        <item name="android:textSize">12sp</item>
        <item name="textAllCaps">true</item>
    </style>

</resources>