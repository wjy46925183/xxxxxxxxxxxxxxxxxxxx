<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/inc_margin_3"/>
            <solid android:color="@color/inc_app_color_grey" />
            <stroke android:width="0.5dp" android:color="@color/inc_app_color_grey"/>
        </shape>
    </item>

    <item>
        <shape>
            <corners android:radius="@dimen/inc_margin_3"/>
            <solid android:color="@color/inc_app_color_white" />
            <stroke android:width="0.5dp" android:color="@color/inc_app_color_grey"/>
        </shape>
    </item>


</selector>